
<h1> Index </h1>

   <% form_with scope: :post, url: posts_path, local: true do |f|%>

      <p>
        <%= f.label :title %><br>
        <%= f.text_field : Username%>
      </p>

      <p>
        <%= f.label :text %><br>
        <%= f.text_field : Image link%>
      </p>

      <p>
        <%= f.submit %>
      </p>

    <% end %>